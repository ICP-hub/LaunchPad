type TokenDetails = record {
  token_symbol : text;
  transfer_fee : nat64;
  minter : principal;
  trigger_threshold : nat64;
  archive_controller : principal;
  pre_minted_tokens : nat64;
  num_of_block_to_archive : nat64;
  cycle_for_archive_creation : nat64;
  token_name : text;
  feature_flags : bool;
  default_owner : principal;
};
service : { create_token : (TokenDetails) -> (text) }
